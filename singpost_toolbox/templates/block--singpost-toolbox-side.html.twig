                    {% set count = 1 %}
                    {% for form in forms %}
                        <div class="track-trace-sec__tab-cont track-trace-sec--search tab-pane {% if count == 1%} active {% endif %}" id="{{ form.title|lower|replace({'/': '',' ': '-', '&': '-'}) }}" role="tabpanel" aria-labelledby="{{ form.title|lower|replace({'/': '',' ': '-', '&': '-'}) }}-tab">
