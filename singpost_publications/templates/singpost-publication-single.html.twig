    <div class="sgp-list-view sgp-mtb-6">
        <div class="content-wrapper-small">
            <div class="sgp-header-arrow">
            {% if(results[0].heading) %}
                <div class="sgp-header-arrow__head-sec">
                    {% if(results[0].sub_heading) %}
                        <p class="sgp-small-title">{{results[0].sub_heading}}</p>
                    {% endif %}
                    <h2 class="sgp-h2">{{results[0].heading}}</h2>
                </div>
            {% endif %}                
            </div>
            <div class="sgp-service__wrapper" id="publications_content">
            {% for value in results[0].content %}
            <div class="sgp-tile sgp-tile--list sgp-load-more__item">
                <div class="sgp-tile__cnt-sec">
                    <div class="sgp-tile__cnt-inner">
                        <h5 class="sgp-h5">{% if(value.name) %}{{value.name}}{% endif %}</h5>
                        {% if(value.list_summary) %}
                        <p class="sgp-tile__desc">{{value.list_summary}}</p>
                        {% endif %}
                    </div>
                    {% if(value.url) %}
                    <a href="{{value.url}}" target="_blank" title="Download Now" class="sgp-btn mt-0">
                        <span class="sgp-btn__icon">
                            <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" viewBox="0 0 30 30">
